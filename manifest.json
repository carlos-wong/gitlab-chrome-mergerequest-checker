{
  "name": "Gitlab Merge Checker",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["http://www.lejuhub.com/*"],
      "js": ["contentScript.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
